<script>

    function recaptchaCallback() {
         if (grecaptcha.getResponse() === '') {
            $(".g-recaptcha").parent().addClass("has-error");
        } else {
            $(".g-recaptcha").parent().removeClass("has-error");
        }
    }
    
    $(function() {
        $("#addParticipant").click(function(){
            var p = $(".participants #participantTemplate").first().clone();
           
            p.find("#participantEmail").attr("required", "true");
            p.find("#participantFullName").attr("required", "true");
            p.find("#participant_remove").click(function()
            {
                $(this).closest(".row").remove();
                return false;
            });

            p.insertBefore("#addParticipant").show();
            return false;
        });

        $("#book-course-form").validate({
            highlight: function(element) {
                $(element).closest('.form-group').addClass('has-error');
            },
            unhighlight: function(element) {
                $(element).closest('.form-group').removeClass('has-error');
            },
            errorPlacement: function(error, element) { },
            ignore: [],
            rules: {
                hiddenRecaptcha: {
                    required: function () {
                        return grecaptcha.getResponse() === '';
                    }
                }
            },
            submitHandler: function(form) {
                form.submit();
            }
        });

        $("#multipleParticipants").click(function() {
            this.checked
                ? $(".participants").show()
                : $(".participants").hide()
        });

        $("#invoiceAddress").click(function() {
            this.checked
                ? $(".invoiceAddress").hide()
                : $(".invoiceAddress").show();
        });

        $(".checkbox").click(function() {
            var cb = $(this).children("input[type=checkbox]")[0];
            var img = $(this).children("img")[0];

            cb.checked
                ? img.src = "/images/checkbox-checked.svg"
                : img.src =  "/images/checkbox-empty.svg";
        });
    });
</script>

            </div>
        </div>

      </div>
    </main>
